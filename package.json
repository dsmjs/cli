{
  "name": "@dsmjs/cli",
  "description": "CLI for our various tools",
  "license": "MIT",
  "version": "0.0.0-semantically-released",
  "bin": {
    "dsmjs": "bin/dsmjs.js"
  },
  "files": [
    "bin/"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": "dsmjs/cli",
  "bugs": "https://github.com/dsmjs/cli/issues",
  "homepage": "https://github.com/dsmjs/cli#readme",
  "author": "dsmJS",
  "maintainers": [
    "Matt Travi <npm@travi.org> (https://matt.travi.org/)"
  ],
  "scripts": {
    "test": "npm-run-all --print-label --parallel lint:* --parallel test:*",
    "test:unit": "nyc run-s test:unit:base",
    "test:unit:base": "DEBUG=any mocha src/**/*-test.js",
    "coverage:report": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "lint:js": "eslint . --cache",
    "lint:md": "remark . --frail",
    "lint:sensitive": "ban",
    "lint:peer": "npm ls >/dev/null",
    "clean": "rimraf ./bin",
    "prebuild": "run-s clean",
    "build": "npm-run-all --print-label --parallel build:*",
    "build:js": "rollup --config",
    "watch": "run-s 'build:js -- --watch'",
    "prepack": "run-s build",
    "lint:gherkin": "gherkin-lint",
    "test:integration": "run-s 'test:integration:base -- --profile noWip'",
    "test:integration:base": "DEBUG=any cucumber-js test/integration --profile base",
    "test:integration:debug": "DEBUG=nock.* run-s test:integration",
    "test:integration:wip": "run-s 'test:integration:base -- --profile wip'",
    "test:integration:focus": "run-s 'test:integration:base -- --profile focus'"
  },
  "dependencies": {
    "@form8ion/dependabot-scaffolder": "1.4.0",
    "@form8ion/github-actions-node-ci": "1.13.2",
    "@form8ion/lift": "1.12.0",
    "@form8ion/lift-javascript": "1.8.2",
    "@form8ion/mocha-scaffolder": "1.0.0",
    "@form8ion/remove-greenkeeper": "1.1.7",
    "@form8ion/replace-travis-ci-with-github-actions": "1.2.5",
    "@travi/github-scaffolder": "5.3.1",
    "@travi/javascript-scaffolder": "12.0.2",
    "@travi/project-scaffolder": "9.0.2",
    "@travi/travis-scaffolder-javascript": "4.0.1",
    "travis-token-updater": "1.6.12",
    "update-notifier": "5.0.1",
    "yargs": "16.2.0"
  },
  "devDependencies": {
    "@babel/register": "7.12.10",
    "@dsmjs/babel-preset": "1.0.52",
    "@dsmjs/eslint-config": "1.0.60",
    "@dsmjs/eslint-config-cucumber": "1.0.1",
    "@dsmjs/eslint-config-mocha": "1.0.11",
    "@dsmjs/remark-preset-lint": "1.0.12",
    "@travi/any": "2.0.15",
    "ban-sensitive-files": "1.9.14",
    "chai": "4.2.0",
    "codecov": "3.8.1",
    "commitlint-config-travi": "1.3.11",
    "cucumber": "6.0.5",
    "cz-conventional-changelog": "3.3.0",
    "debug": "4.3.1",
    "gherkin-lint": "4.1.3",
    "http-status-codes": "2.1.4",
    "husky": "5.0.6",
    "mocha": "8.2.1",
    "mock-fs": "4.13.0",
    "mz": "2.7.0",
    "nock": "13.0.5",
    "npm-run-all": "4.1.5",
    "nyc": "15.1.0",
    "remark-cli": "9.0.0",
    "rimraf": "3.0.2",
    "rollup": "2.35.1",
    "rollup-plugin-auto-external": "2.0.0",
    "rollup-plugin-executable": "1.6.2",
    "rollup-plugin-json": "4.0.0",
    "rollup-plugin-node-resolve": "5.2.0",
    "sinon": "9.2.2",
    "testdouble": "3.16.1"
  }
}
